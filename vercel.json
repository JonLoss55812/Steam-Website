{
  "version": 2,
  "name": "steammn",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "API_URL": "@api_url",
    "RECAPTCHA_KEY": "@recaptcha_key",
    "GOOGLE_MAPS_KEY": "@google_maps_key"
  }
}